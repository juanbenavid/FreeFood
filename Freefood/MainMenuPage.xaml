<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Freefood.MainMenuPage"
             Title="Find My Food">
    <VerticalStackLayout Margin="16" Spacing="32">
        
        <Button
            Text="Find Food"
            x:Name="btn_goToFindFoodPage"
            Clicked="btn_clicked_goToFindFoodPage"/>

        <Button
            Text="List/Donate Food"
            x:Name="btn_goToListFoodPage"
            Clicked="btn_clicked_goToListFoodPage"/>
        
    </VerticalStackLayout>
</ContentPage>